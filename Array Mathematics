import numpy

N,M=map(int,raw_input().split())

A=[]
B=[]
for i in range(2):
    for j in range(N):
        if i==0:
            A.append(list(map(int, raw_input().split())))
        else:
            B.append(list(map(int,raw_input().split())))
A=numpy.array(A,int)
B=numpy.array(B,int) 
print (numpy.add(A,B))
print (numpy.subtract(A,B))
print (numpy.multiply(A,B))
print (numpy.floor_divide(A,B))
print (numpy.mod(A,B))
print (numpy.power(A,B))
